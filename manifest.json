{
    "name": "svenskafans mörkt tema",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "Mörk läge för 'svenskafans.com'",
    "browser_action": {
        "default_title": "Mörk läge svenskafans"
    },
    "permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*.svenskafans.com/*"
            ],
            "css": [
                "/styling/styles.css"
            ]
        }
    ]
}